alias dotfetch='bash <(wget -o /dev/null -O- https://raw.githubusercontent.com/d00ble/dotfiles/master/get.sh)'
alias chat='tmux attach -t weechat'
alias zt='ssh -i ~/.ssh/wporr_rsa -fND 2020 nyx.ocf.berkeley.edu'

# Kubernetes on OCF
alias kctl='kubectl -n dev-wporr'
alias kgd='kubectl -n dev-wporr get deployments'
alias kgs='kubectl -n dev-wporr get services'

alias update-shutdown='sudo pacman -Syu && sudo shutdown now'

